<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".people_cherites">

    <Button
        android:id="@+id/people_cherites_people_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="people"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/people_cherites_cherities_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cherities"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/people_cherites_items_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="items"
        tools:layout_editor_absoluteX="212dp"
        tools:layout_editor_absoluteY="16dp" />

    <ListView
        android:id="@+id/people_cherities_list"
        android:layout_width="304dp"
        android:layout_height="429dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="72dp" />
</android.support.constraint.ConstraintLayout>